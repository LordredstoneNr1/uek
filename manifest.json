{
    "manifest_version": 2,
    "name": "Universe Enhancement Kit",
    "description": "Enhances your League Universe page by adding some functionality.",
    "version": "0.1.0",
    "permissions": ["storage", "contextMenus", "webNavigation"],
    "icons": { 
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/32.png",
        "64": "images/32.png",
        "128": "images/128.png"
        },
    "background": {
        "scripts": ["scripts/background.js", "scripts/data.js"],
        "persistent": false
        },
    "browser_action": {
        "default_popup": "html/popup.html"
    }
}